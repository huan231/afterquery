{
  "name": "afterquery",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "api:dev": "turbo run dev --filter=@afterquery/api...",
    "api:build": "turbo run build --filter=@afterquery/api...",
    "api:start": "turbo run start --filter=@afterquery/api",
    "build": "turbo run build",
    "web:dev": "turbo run dev --filter=@afterquery/web...",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {
    "express": "5.1.0",
    "probot": "14.2.1",
    "octokit": "5.0.4",
    "@afterquery/database": "*",
    "simple-git": "3.28.0",
    "http-proxy-middleware": "3.0.5",
    "resend": "6.3.0",
    "@tsconfig/node22": "22.0.1",
    "@types/express": "5.0.5",
    "@types/node": "22.13.14",
    "drizzle-orm": "0.44.7",
    "postgres": "3.4.7",
    "drizzle-kit": "0.31.6",
    "dotenv": "16.4.7"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@typescript-eslint/parser": "8.26.1",
    "eslint": "9.22.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-config-turbo": "2.4.4",
    "eslint-import-resolver-typescript": "4.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.3",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-turbo": "2.4.4",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "turbo": "2.4.4",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1",
    "tsup": "8.5.0"
  },
  "packageManager": "npm@10.9.2",
  "engines": {
    "node": ">=22"
  }
}
