{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "start": {
      "cache": false,
      "env": ["DATABASE_URL", "GITHUB_PAT", "RESEND_API_KEY", "PUBLIC_URL", "PORT"]
    }
  }
}
